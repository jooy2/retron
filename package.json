{
  "name": "retron",
  "version": "1.0.0",
  "description": "ViteJS + Electron + React + Material-UI Template",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "npm run build:pre && electron-builder -wml",
    "build:pre": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false GENERATE_SOURCEMAP=false NODE_ENV=production vite build",
    "build:win": "npm run build:pre && electron-builder --windows",
    "build:mac": "npm run build:pre && electron-builder --mac",
    "build:linux": "npm run build:pre && electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src"
  },
  "author": "jooy2 <jootc.help@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.3",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "vite": "^3.2.0",
    "vite-plugin-electron": "^0.9.3"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^8.1.0",
    "i18next": "^22.0.2",
    "i18next-browser-languagedetector": "^7.0.0",
    "i18next-http-backend": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
